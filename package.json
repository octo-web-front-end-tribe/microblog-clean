  },
  "scripts": {
    "clean": "rimraf public && mkdir public",
    "build": "npm run clean && webpack -p",
    "start": "webpack-dev-server --progress --inline --hot --quiet --colors --open",
    "lint": "eslint src/**/*.js",
    "mocha": "mocha --reporter spec --compilers js:babel-register -r ignore-styles -r jsdom-global/register --recursive 'src/**/*.spec.js'",
    "test": "npm run lint & npm run mocha",
    "test-watch": "npm run mocha -- --watch"
  },
  "dependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "chai-fetch-mock": "1.0.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "fetch-mock": "5.10.0",
    "html-webpack-plugin": "2.28.0",
    "ignore-styles": "5.0.1",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "rimraf": "2.6.1",
    "sinon": "2.1.0",
